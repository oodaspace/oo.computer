{
  "name": "oo.computer",
  "author": "ooda space ltd <lee@ooda.space>",
  "version": "0.0.103",
  "description": "  The oo.computer is a transparent system to contextualise and rank ideas with others.  It offers those who signal value in ideas and contexts a share of profits that arise from them",
  "homepage": "https://oo.computer",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron --trace-warnings .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -p never",
    "publish": "electron-builder -p always"
  },
  "build": {
    "appId": "oo.computer",
    "win": {
      "target": "nsis",
      "icon": "./icon512wb.png",
      "publish": {
        "provider": "spaces",
        "name": "oodaspace",
        "region": "ams3",
        "acl": "public-read"
      }
    },
    "linux": {
      "target": "deb",
      "publish": {
        "provider": "spaces",
        "name": "oodaspace",
        "region": "ams3",
        "acl": "public-read"
      }
    },
    "mac": {
      "category": "Productivity",
      "target": "default",
      "icon": "./icon512wb.png",
      "publish": {
        "provider": "spaces",
        "name": "oodaspace",
        "region": "ams3",
        "acl": "public-read"
      }
    },
    "deb": {},
    "nsis": {
      "oneClick": true
    },
    "asar": true,
    "files": [
      "!electron-builder.env"
    ]
  },
  "keywords": [],
  "license": "OO",
  "devDependencies": {
    "electron": "^12.0.9",
    "electron-builder": "^22.10.5"
  },
  "dependencies": {
    "@hyperswarm/dht": "^5.0.0-rc.6",
    "cross-blob": "^3.0.0",
    "electron-log": "^4.3.1",
    "electron-updater": "^4.3.5",
    "fetch-blob": "^3.0.1",
    "hyperbee": "^1.6.3",
    "hyperdrive": "^10.20.0",
    "hyperspace": "^3.19.0",
    "it-all": "^1.0.5",
    "pump": "^3.0.0",
    "uint8arrays": "^2.1.6"
  },
  "productName": "oo.computer"
}
